<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.varsha.app1.Main6Activity"

    android:visibility="visible"
    android:background="#FFF0F5">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Time Slots"
        android:id="@+id/mtextviewtimeslots"
        android:layout_alignParentStart="true"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="30dp"
        android:layout_alignParentEnd="true" />

    <RadioButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Time Slot1"
        android:id="@+id/radioButton"
        android:layout_alignTop="@+id/mtextviewtimeslots"
        android:layout_alignParentStart="true"
        android:layout_marginTop="119dp"
        android:checked="false"
        android:clickable="true" />

    <RadioButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Time Slot2"
        android:id="@+id/radioButton2"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true"
        android:clickable="true"
        android:checked="false" />

    <RadioButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Delete"
        android:id="@+id/radioButton3"
        android:layout_below="@+id/radioButton2"
        android:layout_alignParentStart="true"
        android:layout_marginTop="68dp"
        android:clickable="true"
        android:checked="false" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Small Text"
        android:id="@+id/textView28"
        android:layout_alignTop="@+id/radioButton"
        android:layout_alignEnd="@+id/mtextviewtimeslots"
        android:visibility="invisible" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Small Text"
        android:id="@+id/textView29"
        android:layout_below="@+id/radioButton"
        android:layout_alignParentEnd="true"
        android:visibility="invisible" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/mnumber"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/radioButton3"
        android:visibility="invisible" />
</RelativeLayout>
